{
  "name": "react-window-assistant",
  "version": "0.0.7",
  "description": "Helpful utilities to assist a developer",
  "private": false,
  "homepage": "https://windowassistant.netlify.com",
  "author": {
    "email": "johndarrylpelingo@gmail.com",
    "name": "John Darryl Pelingo",
    "url": "https://johndpelingo.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-window-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-window-assistant/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "prettier": "@jdp-dev/prettier-config",
  "scripts": {
    "--DEVELOPMENT--": "echo \"--DEVELOPMENT--\" && exit 1",
    "dev": "react-scripts start",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{css,js,ts,tsx}' '**/*.md'",
    "lint": "eslint ./src/ --ext .js,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
    "postinstall": "husky install",
    "snyk-protect": "snyk protect",
    "sweep": "rimraf build coverage",
    "test": "CI=true react-scripts test --env=jsdom --no-cache",
    "test:ci": "CI=true react-scripts test --env=jsdom --no-cache --coverage",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:w": "react-scripts test --env=jsdom --no-cache --verbose=false",
    "--BUILD--": "echo \"--BUILD--\" && exit 1",
    "build": "react-scripts build",
    "build:ci": "react-scripts build",
    "prebuild": "run-s sweep lint test:t test",
    "prebuild:ci": "run-s sweep lint test:t test:ci",
    "--PRODUCTION--": "echo \"--PRODUCTION--\" && exit 1",
    "release": "standard-version",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "change-case": "^5.0.0",
    "color": "^4.0.1",
    "debounce": "^2.0.0",
    "diff": "^7.0.0",
    "immer": "^10.0.0",
    "lower-case": "^3.0.0",
    "lower-case-first": "^3.0.0",
    "query-string": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "sort-keys": "^5.0.0",
    "styled-components": "^6.0.0",
    "swap-case": "^3.0.0",
    "title-case": "^4.0.0",
    "upper-case": "^3.0.0",
    "upper-case-first": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.0.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@jdp-dev/commitlint-config": "^1.0.0",
    "@jdp-dev/eslint-config-typescript-react": "^1.0.0",
    "@jdp-dev/prettier-config": "^1.0.0",
    "@jdp-dev/tsconfig": "^1.0.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.0.0",
    "@types/color": "^3.0.2",
    "@types/debounce": "^1.2.0",
    "@types/diff": "^5.0.1",
    "@types/history": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.0.0",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-is": "^18.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "codecov": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "9",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^6.0.0",
    "serve": "^14.0.0",
    "snyk": "^1.728.0",
    "standard-version": "^9.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true
}
