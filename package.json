{
  "name": "react-window-assistant",
  "version": "0.0.3",
  "description": "Helpful utilities to assist a developer",
  "private": false,
  "homepage": "https://windowassistant.netlify.com",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-window-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-window-assistant/issues"
  },
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "--DEVELOPMENT--": "echo \"--DEVELOPMENT--\" && exit 1",
    "eject": "react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "posttest": "yarn test:t",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' '**/*.md'",
    "start": "react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "CI=true react-scripts test --env=jsdom --no-cache",
    "test:ci": "CI=true react-scripts test --env=jsdom --no-cache --coverage",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:w": "react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn lint && yarn format:d",
    "--BUILD--": "echo \"--BUILD--\" && exit 1",
    "build": "react-scripts build",
    "build:ci": "react-scripts build",
    "prebuild": "run-s sweep validate test",
    "prebuild:ci": "run-s sweep validate test:ci",
    "--PRODUCTION--": "echo \"--PRODUCTION--\" && exit 1",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "change-case": "^4.1.0",
    "color": "^3.1.2",
    "debounce": "^1.2.0",
    "diff": "^4.0.1",
    "history": "^4.10.1",
    "immer": "^6.0.5",
    "lower-case": "^2.0.1",
    "lower-case-first": "^2.0.1",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sort-keys": "^4.0.0",
    "styled-components": "^5.1.0",
    "swap-case": "^2.0.1",
    "title-case": "^3.0.2",
    "upper-case": "^2.0.1",
    "upper-case-first": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/color": "^3.0.0",
    "@types/debounce": "^1.2.0",
    "@types/diff": "^4.0.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/react-is": "^16.7.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/parser": "^2.31.0",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "serve": "^11.1.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/*.{ts,tsx}",
      "!src/scenes/**/index.ts",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
