{
  "name": "react-window-assistant",
  "version": "0.0.1",
  "description": "Helpful utilities to assist a developer",
  "private": false,
  "homepage": "https://windowassistant.netlify.com",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-window-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-window-assistant/issues"
  },
  "scripts": {
    "--DEVELOPMENT--": "echo \"--DEVELOPMENT--\" && exit 1",
    "eject": "cross-env react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint": "tslint --config tslint.json --project tsconfig.json --force *.{ts,tsx}",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "posttest": "yarn test:t",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' '**/*.md'",
    "start": "cross-env react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "cross-env CI=true react-scripts test --env=jsdom --no-cache",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:w": "cross-env react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn lint && yarn format:d",
    "--BUILD--": "echo \"--BUILD--\" && exit 1",
    "build": "cross-env react-scripts build",
    "build:ci": "yarn build",
    "prebuild": "run-s validate test sweep",
    "--PRODUCTION--": "echo \"--PRODUCTION--\" && exit 1",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@reach/router": "^1.2.1",
    "color": "^3.1.2",
    "immer": "^3.1.3",
    "query-string": "^6.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.1",
    "sort-keys": "^3.0.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.0",
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.18",
    "cross-env": "^5.2.0",
    "husky": "^3.0.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-scripts": "3.1.1",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "serve": "^11.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
