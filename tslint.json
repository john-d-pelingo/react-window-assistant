{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "src/**/*.js"]
  },
  "rules": {
    "interface-name": false,
    "member-access": [true, "no-public"],
    "no-console": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
